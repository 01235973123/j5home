<?xml version="1.0" encoding="utf-8"?>
<extension type="module" client="administrator" method="upgrade" version="3.9.0" position="cpanel">
    <name>Membership Pro - Latest Subscription</name>
    <author>Tuan Pham Ngoc</author>
    <creationDate>November 07 2022</creationDate>
    <copyright>Copyright 2012 - 2025 Ossolution Team</copyright>
    <authorEmail>tuanpn@joomdonation.com</authorEmail>
    <authorUrl>https://joomdonation.com</authorUrl>
    <version>4.2.2</version>
    <description>Display Latest Subscriptions from Membership Pro.</description>
    <namespace path="src">OSSolution\MembershipPro\Module\Subscriptions</namespace>
    <files>
        <folder module="mod_mp_subscriptions">services</folder>
        <folder>src</folder>
        <folder>tmpl</folder>
    </files>
    <config>
        <fields name="params">
            <fieldset name="basic">
                <field
                        name="count"
                        type="number"
                        label="Number Subscriptions"
                        description="Number Subscriptions which will be displayed in the module"
                        default="10"
                        filter="integer"
                />
            </fieldset>
        </fields>
    </config>
</extension>
